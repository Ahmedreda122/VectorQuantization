<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="286.0" prefWidth="210.0" spacing="20.0" style="-fx-background-color: aqua;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vectorquan.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Vector Quantization" />
   <TextField fx:id="nCodebooks" promptText="Number of codebooks" />
   <TextField fx:id="CDWidth" promptText="Codebook width" />
   <TextField fx:id="CDHeight" promptText="Codebook height" />
    <Button onAction="#onCompressButtonClick" prefWidth="69.0" text="Compress" />
   <Button onAction="#onDecompressButtonClick" text="Decompress" />

    <Label fx:id="welcomeText" />
</VBox>
